    <Directory>Prism.SMAA</Directory>
    <Directory>Prism.Maths</Directory>
    <Directory>Prism.Common</Directory>
  </SourceDirectories>
  
  <!-- To specify asset files to be copied onto disk, use the AssetFolder tag. -->
  <AssetFolder>Prism.SMAA/Assets/</AssetFolder>
</PluginData>
